{
  "name": "nidhoggbot",
  "version": "2.1.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:clear": "node --require ts-node/register ./node_modules/typeorm/cli.js query \"DROP SCHEMA public CASCADE; CREATE SCHEMA public; GRANT USAGE ON SCHEMA public to PUBLIC; GRANT CREATE ON SCHEMA public to PUBLIC; COMMENT ON SCHEMA public IS 'standard public schema';\"",
    "migration:create": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:create -f \"./ormconfig.json\" -n migration",
    "migration:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate -f \"./ormconfig.json\" -n migration",
    "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js -f \"./ormconfig.json\" migration:run",
    "ormconfig:generate": "ts-node ./src/configuration/ormconfig.ts",
    "docker-compose:run": "docker-compose up -d ",
    "docker-compose:rebuild": "docker-compose up -d -V --build",
    "docker-compose:restart": "docker-compose down && docker-compose up -d ",
    "docker-compose:full-restart": "docker-compose down && docker rm -f $(docker ps -a -q) && docker volume rm $(docker volume ls -q) && docker-compose up -d",
    "docker-compose:down": "docker-compose down"
  },
  "dependencies": {
    "@binance/connector": "^1.5.0",
    "@discordjs/opus": "^0.5.3",
    "@discordjs/voice": "^0.7.5",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.3",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.1.4",
    "@nestjs/typeorm": "^8.0.2",
    "argon2": "^0.28.2",
    "axios": "^0.24.0",
    "bull": "^3.29.3",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "discord-phub": "^1.0.6",
    "discord.js": "^13.3.1",
    "discordjs-ytdl": "^2.2.0",
    "dotenv": "^10.0.0",
    "epic-free-games": "^2.1.5",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^4.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "hmfull": "^1.3.0",
    "lodash": "^4.17.21",
    "mongodb": "^3.7.3",
    "node-fetch": "^3.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "play-dl": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "tmp-promise": "^3.0.3",
    "typeorm": "^0.2.38",
    "youtube-dl-exec": "^1.2.4",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.5",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.177",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/request": "^2.48.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
